<ResourceDictionary
    x:Class="LucasAlias.NINA.NEK.Drivers.NikonFocuserNekSettings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ninactrl="clr-namespace:NINA.CustomControlLibrary;assembly=NINA.CustomControlLibrary">
    <DataTemplate x:Key="LucasAlias.NINA.NEK.Drivers.NikonCameraNek+NikonFocuserNek_FocuserSettings">
        <Grid>
            <StackPanel Orientation="Vertical">
                <ninactrl:CancellableButton ButtonText="Calibrate" Command="{Binding StartCalibration}" CancelCommand="{Binding CancelCalibration}" CancelButtonImage="{StaticResource CancelSVG}"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
</ResourceDictionary>